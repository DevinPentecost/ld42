script/source = "extends VBoxContainer

signal ViewBioSignal(dogbio)

var _wiggleDuration = 1.0
var _wiggleCur = 0.0

func ViewBio(dogbio):
	self.emit_signal(\"ViewBioSignal\", dogbio)
	self.visible = true
	_wiggleCur = 0.0

func HideBio():
	self.visible = false

func _ready():
	_wiggleCur = _wiggleDuration
	
func _process(delta):
	_wiggleCur = _wiggleCur + delta
	if _wiggleCur >= _wiggleDuration:
		self.rect_rotation = 0.0
		return
	self.rect_rotation = cos(_wiggleCur * 8.0) * 2.0"
